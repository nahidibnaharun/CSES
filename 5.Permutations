#include <iostream>
 
using namespace std;
 
int main()
{
 int n;
 cin>>n;
 if(n<4 && n>1) cout<<"NO SOLUTION";
 else if(n==4) cout<<"2 4 1 3";
 else{
     long long d=n;
         while(d>0){
             cout<<d<<" ";
             d-=2;
         }
         n--;
         while(n>0) {
             cout<<n<<" ";
             n-=2;
         }
 }
 
    return 0;
}
